{
  "icon": "assets/terraform_logo.png",
  "name": "easy-terraform-commands",
  "displayName": "Terraform Dashboard Plus",
  "publisher": "oferkafry",
  "description": "Supercharge your Terraform journey with a control center. Say goodbye to tedious typing as this extension brings effortless, UI-driven execution and real-time monitoring to Terraform commands.",
  "version": "2.5.9",
  "engines": {
    "vscode": "^1.67.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oferca/vscode-ext-tf.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js", 
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "terraform-dashboard",
          "title": "terraform dashboard",
          "icon": "resources/package-explorer.svg",
          "order": 2
        }
      ]
    },
    "views": {
      "terraform-dashboard": [
        {
          "id": "terraform-dashboard",
          "name": "terraform-dashboard-d"
        }
      ],
			"explorer": [
				{
					"type": "webview",
					"id": "terraform-button-view-explorer",
					"name": "Terraform"
				}
			],
      "scm": [
				{
					"type": "webview",
					"id": "terraform-button-view-scm",
					"name": "Terraform"
				}
			]
		},
    "commands": [
      {
        "command": "vscode-ext-tf.tfcommand",
        "title": "Run Terraform Command"
      }
    ],
    "keybindings": [
      {
        "command": "terraform.action",
        "key": "cmd+shift+t"
      },
      {
        "command": "terraform.action",
        "key": "ctrl+shift+t"
      }
    ]
  },
  "categories": [
    "Programming Languages",
    "Other",
    "Visualization",
    "Linters",
    "Testing"
  ],
  "keywords": [
    "devops",
    "terraform",
    "tf",
    "hashicorp",
    "apply",
    "plan",
    "buttons",
    "output"
  ],
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "dependencies": {
    "find-remove": "^4.1.0",
    "firebase": "^9.21.0",
    "readline": "^1.3.0",
    "vscode-uri": "^3.0.7"
  }
}
